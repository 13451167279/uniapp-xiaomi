<view class="goodsFloor"><block wx:if="{{cateGoods.product_list.length>0}}"><view class="gootsList"><view class="goodsTitle">{{cateGoods.title}}</view><block wx:for="{{cateGoods.product_list}}" wx:for-item="product" wx:for-index="__i0__" wx:key="*this"><view data-event-opts="{{[['tap',[['toDetail',[['o',['product',product]]]]]]]}}" class="goods" bindtap="__e"><image src="{{product.puzzle_url}}" mode></image><view class="goodsInfo"><view class="name">{{product.name}}</view><view class="sellInfo"><text class="price">{{product.price}}</text><block wx:if="{{product.labels.length>0}}"><text class="tag">{{product.labels[0]}}</text></block></view></view></view></block></view></block><block wx:else><view class="goodsFloor_1"><view class="title">{{cateGoods.title}}</view><view class="goodsList"><block wx:for="{{cateGoods.items}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="goods"><image src="{{item.img_url}}" mode></image><text class="name">{{item.product_name}}</text></view></block></view></view></block></view>